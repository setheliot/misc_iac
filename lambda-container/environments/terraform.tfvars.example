# ===== terraform.tfvars.example =====
# Copy this to terraform.tfvars and fill in your values
#
# Select ECR container image
# ecr_container_image = "<repository>:<tag>, for example: guestbook-app:lambda"

# Set AWS Region
# aws_region          = "us-east-1"

# These are optional
# environment         = "dev"
# project_name        = "guestbook"
# dynamodb_table_name = "guestbook-entries"
# lambda_memory_size  = 512
# lambda_timeout      = 30